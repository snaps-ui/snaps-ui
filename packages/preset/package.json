{
  "name": "@snaps-ui/panda-preset",
  "version": "0.0.0-development",
  "description": "Snaps UI Panda CSS Preset",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js",
      "require": "./dist/utils/index.cjs"
    },
    "./colors/*": {
      "types": "./dist/colors/*.d.ts",
      "import": "./dist/colors/*.js",
      "require": "./dist/colors/*.cjs"
    },
    "./anatomy": {
      "types": "./dist/anatomy.d.ts",
      "import": "./dist/anatomy.js",
      "require": "./dist/anatomy.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "clean": "rimraf --no-glob ./dist",
    "build": "pnpm clean && tsup --config tsup.config.ts && tsc --outDir dist --project tsconfig.json",
    "prebuild": "pnpm clean",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx --config ../../eslint.config.js",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/snaps-ui/snaps-ui",
    "directory": "packages/presets"
  },
  "keywords": [
    "Snaps UI",
    "presets",
    "panda css",
    "Ark UI"
  ],
  "author": "Justice Chimobi <chimobi.justice@gmail.com>",
  "license": "MIT",
  "packageManager": "pnpm@10.15.0",
  "peerDependencies": {
    "@pandacss/dev": ">=1.1.0"
  },
  "devDependencies": {
    "@pandacss/dev": "^1.3.1",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@ark-ui/react": "^5.25.0"
  }
}
