{
  "name": "@snaps-ui/react",
  "version": "0.0.0-development",
  "private": false,
  "description": "A collection of prebuilt, customizable Chakra UI components for faster development",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./base-presets": {
      "types": "./dist/base-presets.d.ts",
      "import": "./dist/base-presets.js",
      "require": "./dist/base-presets.cjs"
    },
    "./hooks": {
      "types": "./dist/hooks/index.d.ts",
      "import": "./dist/hooks/index.js",
      "require": "./dist/hooks/index.cjs"
    },
    "./collection": {
      "types": "./dist/collection.d.ts",
      "import": "./dist/collection.js",
      "require": "./dist/collection.cjs"
    },
    "./provider": {
      "types": "./dist/provider/index.d.ts",
      "import": "./dist/provider/index.js",
      "require": "./dist/provider/index.cjs"
    },
    "./*": {
      "types": "./dist/components/*/index.d.ts",
      "import": "./dist/components/*/index.js",
      "require": "./dist/components/*/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "clean": "rimraf --no-glob ./dist",
    "build:tsup": "NODE_OPTIONS='--max-old-space-size=16384' tsup --config tsup.config.ts ",
    "build:types": "tsc --emitDeclarationOnly --outDir dist --project tsconfig.json",
    "prebuild": "pnpm clean",
    "build": "tsc --outDir dist --project tsconfig.json",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx --config ../../eslint.config.js",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/snaps-ui/snaps-ui",
    "directory": "packages/snaps-ui"
  },
  "keywords": [
    "snaps-ui",
    "react",
    "ui",
    "chakra-ui",
    "design-system",
    "react-components",
    "accessible",
    "uikit",
    "components",
    "library",
    "emotion"
  ],
  "author": "Justice Chimobi <chimobi.justice@gmail.com>",
  "license": "MIT",
  "packageManager": "pnpm@10.15.0",
  "peerDependencies": {
    "@chakra-ui/react": "^3.26.0",
    "@emotion/react": "^11",
    "next-themes": "^0.4.6",
    "react": ">=18",
    "react-dom": ">=18"
  },
  "dependencies": {
    "@snaps-ui/system": "workspace:*",
    "sonner": "^2.0.7"
  },
  "devDependencies": {
    "@chakra-ui/react": "^3.26.0",
    "@emotion/react": "^11",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-icons": "^5.5.0",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
